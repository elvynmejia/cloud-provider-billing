{
  "name": "take-home",
  "version": "1.0.0",
  "description": "take home assessment",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && npm link morgan && node compiled/web-service/app/server.js",
    "test-web-service": "mocha -r ts-node/register web-service/spec/**/*.spec.ts --reporter spec --require web-service/spec/index.ts --exit",
    "noto-web-service": "mocha -r ts-node/register web-service/spec/**/*.spec.ts --reporter spec --require web-service/spec/index.ts --no-timeout --exit",
    "test-client": "mocha -r ts-node/register client/spec/**/*.spec.ts --reporter spec --exit",
    "lint": "eslint web-service/app/**/*.ts web-service/spec/**/*.spec.ts client/**/*.ts client/spec/**/*.ts",
    "lint-fix": "eslint web-service/app/**/*.ts web-service/spec/**/*.spec.ts  client/**/*.ts client/spec/**/*.ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cors": "^2.8.13",
    "@types/mocha": "^10.0.1",
    "@types/uuid": "^9.0.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "mocha": "^10.2.0"
  },
  "dependencies": {
    "@json2csv/plainjs": "^6.1.2",
    "@types/express": "^4.17.15",
    "@types/http-errors": "^2.0.1",
    "@types/json2csv": "^5.0.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.17",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "http-errors": "^2.0.0",
    "mongoose": "^7.2.0",
    "redis": "^4.6.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0"
  }
}
